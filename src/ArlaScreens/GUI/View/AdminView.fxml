<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="1250.0" stylesheets="@../CSS/Admin.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ArlaScreens.GUI.Controller.AdminViewController">
    <children>
        <MenuBar layoutY="1.0" prefHeight="27.0" prefWidth="1280.0" AnchorPane.topAnchor="0.0">
            <menus>
                <Menu mnemonicParsing="false" text="User Settings">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#addUserAction" text="Add User" />
                        <MenuItem mnemonicParsing="false" onAction="#editUserAction" text="Edit User" />
                        <MenuItem mnemonicParsing="false" onAction="#deleteUserAction" text="Delete User" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <TableView fx:id="departmentTableView" layoutY="66.0" prefHeight="551.0" prefWidth="600.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0">
            <columns>
                <TableColumn fx:id="departmentCol" prefWidth="100.0" text="Afdeling" />
                <TableColumn prefWidth="100.0" text="Info 1" />
                <TableColumn prefWidth="100.0" text="Info 2" />
                <TableColumn prefWidth="100.0" text="Info 3" />
                <TableColumn prefWidth="100.0" text="Info 4" />
                <TableColumn prefWidth="100.0" text="Info 5" />
            </columns>
        </TableView>
        <Label layoutX="28.0" layoutY="37.0" prefHeight="14.0" prefWidth="160.0" text="Produktions brugere" AnchorPane.leftAnchor="28.0" AnchorPane.topAnchor="37.0" />
        <CheckBox layoutX="750.0" layoutY="78.0" mnemonicParsing="false" onAction="#barChartChoiceBox" prefHeight="30.0" prefWidth="160.0" text="Søjle Diagram">
         <font>
            <Font size="18.0" />
         </font></CheckBox>
        <CheckBox layoutX="750.0" layoutY="128.0" mnemonicParsing="false" onAction="#pieChartChoiceBox" prefHeight="30.0" prefWidth="160.0" text="Cirkel Diagram" textFill="#111111">
         <font>
            <Font size="18.0" />
         </font></CheckBox>
        <CheckBox layoutX="750.0" layoutY="178.0" mnemonicParsing="false" onAction="#showWebsiteChoiceBox" prefHeight="30.0" prefWidth="140.0" text="Hjemmeside" textFill="#0d0d0d">
         <font>
            <Font size="18.0" />
         </font></CheckBox>
        <CheckBox layoutX="750.0" layoutY="228.0" mnemonicParsing="false" onAction="#showPDFChoiceBox" prefHeight="30.0" prefWidth="84.0" text="PDF" textFill="#0a0a0a">
         <font>
            <Font size="18.0" />
         </font></CheckBox>
        <Button layoutX="820.0" layoutY="230.0" mnemonicParsing="false" onAction="#handleChoosePDFBtn" stylesheets="@../CSS/Admin.css" text="Vælg PDF" />
        <Button layoutX="908.5" layoutY="600.0" mnemonicParsing="false" onAction="#handleSaveBtn" text="Gem" />
        <Button layoutX="850.0" layoutY="600.0" mnemonicParsing="false" onAction="#handleRegretBtn" text="Fortryd" />
        <Label fx:id="choosedDepLabel" layoutX="1268.0" layoutY="66.0" prefHeight="17.0" prefWidth="167.0" />
      <CheckBox layoutX="750.0" layoutY="278.0" mnemonicParsing="false" onAction="#showPDFChoiceBox" prefHeight="30.0" prefWidth="84.0" text="CSV" textFill="#111111">
         <font>
            <Font size="18.0" />
         </font>
      </CheckBox>
      <ListView layoutX="750.0" layoutY="328.0" prefHeight="100.0" prefWidth="200.0" />
      <Button layoutX="820.0" layoutY="280.0" mnemonicParsing="false" onAction="#handleChoosePDFBtn" text="Vælg CSV" />
    </children>
</AnchorPane>
